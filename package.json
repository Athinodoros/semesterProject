    "gulp-csslint": "0.3.0",
    "gulp-env": "0.4.0",
    "gulp-esdoc": "0.3.0",
    "gulp-eslint": "2.0.0",
    "gulp-istanbul": "0.10.4",
    "gulp-live-server": "0.0.30",
    "gulp-mocha": "2.2.0",
    "gulp-ng-annotate": "2.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-strip-comments": "2.4.3",
