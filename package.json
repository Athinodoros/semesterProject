{
  "name": "semesterproject",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production ./server/bin/www",
    "test": "cross-env NODE_ENV=test mocha test --recursive",
    "dev": "cross-env NODE_ENV=test nodemon ./server/bin/www",
    "test:all": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec test --recursive ",
    "precommit": "npm test && npm run test:all &&  npm run check-coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov ",
    "check-coverage": "istanbul check-coverage --functions 20"
  },
  "cacheDirectories": [
    "node_modules",
    "public/bower_components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AwesomeTeamCph/semesterProject.git"
  },
  "keywords": [
    "Node",
    "Angular",
    "Mongo",
    "Neo4J",
    "Express",
    "Awesome"
  ],
  "dependencies": {
    "async": "^2.4.0",
    "body-parser": "~1.16.0",
    "bower": "1.8.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "del": "2.2.2",
    "ejs": "2.5.6",
    "express": "~4.14.1",
    "gulp": "3.9.1",
    "gulp-clean-css": "3.2.0",
    "hbs": "~4.0.1",
    "husky": "^0.13.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "mongodb": "^2.2.26",
    "mongoose": "4.9.8",
    "morgan": "~1.7.0",
    "promise": "^7.1.1",
    "run-sequence": "1.2.2",
    "serve-favicon": "~2.3.2",
    "sinon": "^2.2.0",
    "tunnel-ssh": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-3": "6.16.0",
    "babel-register": "6.16.0",
    "babel-runtime": "6.11.6",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.1",
    "cross-env": "5.0.0",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-autoprefixer": "3.1.0",
    "gulp-babel": "6.1.2",
    "gulp-babel-istanbul": "1.3.1",
    "gulp-bytediff": "1.0.0",
    "gulp-codecov": "2.0.1",
