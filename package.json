{
  "name": "semesterproject",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&& nodemon ./bin/www",
    "test": "set NODE_ENV=test&& mocha test --recursive",
    "test:all": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec test --recursive ",
    "precommit": "npm test && npm run test:all &&  npm run check-coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov ",
    "check-coverage": "istanbul check-coverage --functions 80"
  },
  "dependencies": {
    "async": "^2.4.0",
    "body-parser": "~1.16.0",
    "bower": "1.8.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "2.5.6",
    "express": "~4.14.1",
    "hbs": "~4.0.1",
    "husky": "^0.13.3",
    "mocha": "^3.3.0",
    "mongodb": "^2.2.26",
    "morgan": "~1.7.0",
    "promise": "^7.1.1",
    "serve-favicon": "~2.3.2",
    "sinon": "^2.2.0",
    "tunnel-ssh": "^4.1.2"
  },
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "nyc": "^10.3.2"
  }
}
